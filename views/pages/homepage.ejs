<%- include('../partials/header') %>

<div class="home-page">
  <h2>All Facespace members</h2>
  <ul class="homepage__all">
    <% users.forEach(item => { %>
      <a href="/users/<%= item._id %>">
        <li class="homepage__all--user">
          <img src="<%= item.avatarUrl %>">
        </li>
      </a>
    <% }) %>
  </ul>
</div>

<%- include('../partials/footer') %>